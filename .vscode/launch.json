{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Serve Docs",
            "type": "PowerShell",
            "cwd": "${workspaceFolder}",
            "request": "launch",
            "script": "task docs:serve",
        },
        {
            "name": "Dry Run",
            "type": "debugpy",
            "cwd": "${workspaceFolder}",
            "request": "launch",
            "program": "generate.py",
            "args": [
                "new",
                "--path",
                ".venv/testing/dry",
                "--project-name",
                "something",
                "--author",
                " me",
                "--repo-url",
                "github.com",
                "--repo-name",
                "something/fun",
                "--repo-remote-url",
                "git@github.com",
                "--dry-run",
                "--repo-docs-url",
                "example.com",
                "--contact-email",
                "contact@gmail",
                "--security-email",
                "security@gmail",
            ],
        },
        {
            "name": "Non-Dry Run",
            "type": "debugpy",
            "cwd": "${workspaceFolder}",
            "request": "launch",
            "program": "generate.py",
            "args": [
                "new",
                "--path",
                ".venv/testing/dry",
                "-y",
                "--project-name",
                "something",
                "--author",
                " me",
                "--repo-url",
                "github.com",
                "--repo-name",
                "something/fun",
                "--repo-remote-url",
                "git@github.com",
                "--repo-docs-url",
                "example.com",
                "--contact-email",
                "contact@gmail",
                "--security-email",
                "security@gmail",
            ],
        }
    ]
}
